import java.util.Scanner;


public class dsd {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int hours = 0;
		int minutes = 0;
		for (int i = 0; i < 999; i++) {
			hours = hours + input.nextInt();
			minutes = minutes + input.nextInt();
			if (input.next() == "End") {
				break;
			}
		}
		System.out.printf("%d:%d", hours, minutes);

	}

}
