import java.util.Scanner;

public class VideoDurations {

	public static void main(String[] args) {

		Scanner input = new Scanner(System.in);
		String[] inputs =new String[999];
		int hours = 0;
		
	for (int i = 0; i < 999; i++) {
		inputs=input.next().split(":");
		hours+=input.nextInt(); 
		System.out.println(inputs);
	}
	}

}
