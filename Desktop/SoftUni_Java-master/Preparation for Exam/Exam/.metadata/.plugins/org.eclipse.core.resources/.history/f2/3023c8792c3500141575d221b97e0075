import java.util.Scanner;

public class VideoDurations {

	public static void main(String[] args) {

		Scanner input = new Scanner(System.in);
		String[] inputs= new String[999];
		String regInput[] =new String[999];
		int hours =0;
		int minutes = 0;
		String regInput ="";
		
		for (int i = 0; i < inputs.length; i++) {
			 inputs[i] =input.nextLine().replaceAll(":"," ");
			 regInput[i] =""+ inputs[i].split(" ");
		}
		
		
		
	}

}
