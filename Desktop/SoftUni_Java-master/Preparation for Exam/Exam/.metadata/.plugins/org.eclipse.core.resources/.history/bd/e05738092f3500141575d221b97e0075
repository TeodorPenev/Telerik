import java.util.Scanner;

public class VideoDurations {

	public static void main(String[] args) {

		Scanner input = new Scanner(System.in);
		String[] inputs= new String[999];
		String regInput[] =new String[999];
		int hours =0;
		int minutes = 0;
		
		
		for (int i = 0; i < inputs.length; i++) {
			 inputs[i] =input.next().replaceAll(":", " ");
			 if (inputs[i].equals("End")) {
				break;
			}
		}
		
		for (int i = 0; i < regInput.length; i++) {
			hours = hours + Integer.parseInt(inputs[i]);
		}
		
		
	}

}
